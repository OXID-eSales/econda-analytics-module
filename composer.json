{
    "name": "oxid-esales/econda-analytics-module",
    "description": "This is the Econda analytics module for the OXID eShop.",
    "type": "oxideshop-module",
    "keywords": ["oxid", "modules", "eShop"],
    "homepage": "https://www.oxid-esales.com",
    "license": [
        "proprietary"
    ],
    "extra": {
        "oxideshop": {
            "blacklist-filter": [
                "documentation/**/*.*",
                "composer.json",
                "README.md",
                "LICENSE",
                "CHANGELOG.md"
            ],
            "target-directory": "oe/econdaanalytics"
        }
    },
    "require": {
        "php": ">=7.0",
        "webmozart/path-util": "^2.3",
        "gargron/fileupload": "^1.4.5",
        "symfony/filesystem": "^3.0",
        "econda/tracking": "^3.0.1",
        "league/csv": "^9.1.4"
    },
    "autoload": {
        "psr-4": {
            "OxidEsales\\EcondaAnalyticsModule\\": "../../../source/modules/oe/econdaanalytics"
        }
    }
}
